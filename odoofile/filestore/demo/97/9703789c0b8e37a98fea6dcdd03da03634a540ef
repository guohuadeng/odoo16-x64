if(env.isSmall){if(!view.isMobileFriendly){view=_findMobileView(views,view.multiRecord)||view;}
if(lazyView&&!lazyView.isMobileFriendly){lazyView=_findMobileView(views,lazyView.multiRecord)||lazyView;}}
