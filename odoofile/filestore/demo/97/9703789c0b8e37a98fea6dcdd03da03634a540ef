break;}
case"arrowright":{if(tagIndex===tags.length-1){input.focus();}else{this.focusTag(tagIndex+1);}
break;}
case"backspace":{input.focus();const{id}=this.tags[tagIndex]||{};this.deleteTag(id);break;}
default:return;}
ev.preventDefault();ev.stopPropagation();}}
