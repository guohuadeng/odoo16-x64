var sign=Math.sign(number);number=Math.abs(number);for(var i=val.length;i>0;i--){var s=Math.pow(10,i*3);if(s<=number/Math.pow(10,minDigits-1)){number=Math.round(number*d2/s)/d2;symbol=val[i-1];break;}}
