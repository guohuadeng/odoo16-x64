/* /bus/static/src/im_status_service.js */
odoo.define('@bus/im_status_service',async function(require){'use strict';let __exports={};const{browser}=require('@web/core/browser/browser');const{registry}=require('@web/core/registry');const UPDATE_BUS_PRESENCE_DELAY=__exports.UPDATE_BUS_PRESENCE_DELAY=60000;const imStatusService=__exports.imStatusService={dependencies:['bus_service','multi_tab','presence'],start(env,{bus_service,multi_tab,presence}){const imStatusModelToIds={};let updateBusPresenceTimeout;const throttledUpdateBusPresence=_.throttle(function updateBusPresence(){clearTimeout(updateBusPresenceTimeout);if(!multi_tab.isOnMainTab()){return;}
