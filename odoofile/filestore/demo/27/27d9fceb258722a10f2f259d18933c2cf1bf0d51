if(!lastGroup){lastGroup=[cur];acc.push(lastGroup);}
lastValue=curVal;return acc;},[]);},patch:function(obj,patchName,patch){if(!patchMap.has(obj)){patchMap.set(obj,{original:{},patches:[],});}
