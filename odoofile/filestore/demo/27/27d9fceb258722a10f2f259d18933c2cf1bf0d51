__exports.prepareUpdate=prepareUpdate;function prepareUpdate(...args){const positions=[...args];const restoreData=[];let el,offset;while(positions.length){offset=positions.pop();el=positions.pop();const left=getState(el,offset,DIRECTIONS.LEFT);restoreData.push(left);restoreData.push(getState(el,offset,DIRECTIONS.RIGHT,left.cType));}
